@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>AddPage</title>
    <meta content="text/html;charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta http-equiv="Pragma" content="no-cache" />
    <meta http-equiv="Cache-Control" content="no cache" />
    <meta http-equiv="Expires" content="0" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="format-detection" content="telephone=no" />
    <script src="https://cdn.bootcss.com/pace/1.0.2/pace.min.js"></script>
    <link href="https://cdn.bootcss.com/pace/1.0.2/themes/pink/pace-theme-flash.css" rel="stylesheet">

    <link rel="stylesheet" type="text/css" href="/MyFslayui/plugins/layui/css/layui.css" media="all" />
    <link rel="stylesheet" type="text/css" href="/MyFslayui/css/fs.css" media="all" />
    <script type="text/javascript" src="/MyFslayui/plugins/layui/layui.js"></script>
    <script type="text/javascript" src="/MyFslayui/plugins/jquery/jquery.min.js"></script>
    <script type="text/javascript" src="/MyFslayui/plugins/frame/js/fsDict.js?v=1.7.0"></script>
    <script type="text/javascript" src="/MyFslayui/plugins/frame/js/fs.js?v=1.7.0"></script>
    <script type="text/javascript" src="/MyFslayui/plugins/frame/js/frame.js?v=1.7.0"></script>
</head>
<body>
    <div class="layui-fluid">

        <!--表单开始 start -->
        <form class="layui-form">

            <div class="layui-form-item">
                <label class="layui-form-label">班级</label>
                <div class="layui-input-inline" style="width: 100px;">
                    <div class="layui-input-inline">
                        <select name="StudentList" lay-verify="required" lay-vertype="tips" class="fsDict" dict="StudentList" addnull="1"></select>
                    </div>
                </div>
            </div>

            <div class="layui-form-item">
                <label class="layui-form-label">安排理论课次数</label>
                <div class="layui-input-block">
                    <input type="number" name="AnPaiLiLunKeCiShu" required="" lay-vertype="tips" lay-verify="required"
                           placeholder="请输入理论课次数" autocomplete="off" class="layui-input" value="4" />
                </div>
            </div>

            <div class="layui-form-item">
                <label class="layui-form-label">安排自习课次数</label>
                <div class="layui-input-inline" style="width: 100px;">
                    <input type="number" name="AnPaiZiXiKeCiShu" required="" lay-vertype="tips" lay-verify="required"
                           placeholder="请输入自习课次数" autocomplete="off" class="layui-input" value="4" />
                </div>
            </div>
            <div class="layui-form-item">
                <label class="layui-form-label">安排其他课次数</label>
                <div class="layui-input-inline" style="width: 100px;">
                    <input type="text" name="ShenHeRen" required="" lay-vertype="tips" lay-verify="required"
                           placeholder="请输入自习课次数" autocomplete="off" class="layui-input" value="0" />
                </div>
            </div>
            <hr />
            <div class="layui-form-item" style="text-align: center;">
                <button class="layui-btn" lay-submit="" lay-filter="save" url="/BanJiMoRenShezi/AddAction">新增</button>
                <button type="button" class="layui-btn layui-btn-primary" function="close">关闭</button>
            </div>
        </form>
        <!--表单开始 end -->
    </div>
</body>
</html>
