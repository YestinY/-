@{
    Layout = null;
}
@model List<Models.BanJiBiao>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>添加学员信息</title>
    <meta content="text/html;charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta http-equiv="Pragma" content="no-cache" />
    <meta http-equiv="Cache-Control" content="no cache" />
    <meta http-equiv="Expires" content="0" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="format-detection" content="telephone=no" />
    <script src="https://cdn.bootcss.com/pace/1.0.2/pace.min.js"></script>
    <link href="https://cdn.bootcss.com/pace/1.0.2/themes/pink/pace-theme-flash.css" rel="stylesheet">

    <link rel="stylesheet" type="text/css" href="/MyFslayui/plugins/layui/css/layui.css" media="all" />
    <link rel="stylesheet" type="text/css" href="/MyFslayui/css/fs.css" media="all" />
    <script type="text/javascript" src="/MyFslayui/plugins/layui/layui.js"></script>
    <script type="text/javascript" src="/MyFslayui/plugins/jquery/jquery.min.js"></script>
    <script type="text/javascript" src="/MyFslayui/plugins/frame/js/fsDict.js?v=1.7.0"></script>
    <script type="text/javascript" src="/MyFslayui/plugins/frame/js/fs.js?v=1.7.0"></script>
    <script type="text/javascript" src="/MyFslayui/plugins/frame/js/frame.js?v=1.7.0"></script>
</head>
<body>
    <div class="layui-fluid">

        <!--表单开始 start -->
        <form class="layui-form">
            <div class="layui-form-item">
                <label class="layui-form-label">班级：</label>
                <div class="layui-input-block">
                    <select name="BanJiMing" lay-verify="required">
                        <option value="-1">--不限--</option>
                        @foreach (var a in Model)
                        {
                            <option value="@a.ID">@a.BanJiMing</option>
                        }
                    </select>
                </div>
            </div>



            <div class="layui-form-item layui-form-text">
                <label class="layui-form-label">学员信息</label>
                <div class="layui-input-inline">
                    <input type="text" id="filePath" name="filePath" autocomplete="off" disabled="disabled" class="layui-input" lay-vertype="tips" lay-verify="required" />
                </div>
                <div class="layui-input-inline">
                    <button type="button" class="layui-btn" function="upload" fileelem="#filePath" fileaccept="file" fileexts="xls" filesize="10240" inputs="type:test">上传信息</button>
                </div>
            </div>

            <hr />
            <div class="layui-form-item" style="text-align: center;">
                <button class="layui-btn" lay-submit="" lay-filter="save" url="/ZM/StuAddAction">新增</button>
                <button type="button" class="layui-btn layui-btn-primary" function="close">关闭</button>
            </div>
        </form>
        <!--表单开始 end -->
    </div>
</body>
</html>
